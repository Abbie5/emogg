{
  "schemaVersion": 1,
  "id": "emogg",
  "version": "${version}",
  "name": "emogg",
  "description": "Adds emoji support to the chat and to most of the Minecraft interface",
  "authors": [
    "Arataki Leo"
  ],
  "contact": {
    "repo": "https://github.com/teaco/emogg"
  },
  "license": "GPL-3.0",
  "icon": "assets/emogg/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "pextystudios.emogg.Emogg"
    ]
  },
  "mixins": [
    "emogg.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "accessWidener" : "emogg.accesswidener"
}
