{
  "schemaVersion": 1,
  "id": "emogg",
  "version": "${version}",
  "name": "emogg",
  "description": "The mod draws and adds emojis in the text of the entire minecraft interface",
  "authors": [
    "Arataki Leo"
  ],
  "contact": {
    "repo": "https://github.com/teaco/emogg"
  },
  "license": "GPL-3.0",
  "icon": "assets/emogg/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "pextystudios.emogg.Emogg"
    ]
  },
  "mixins": [
    "emogg.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
